{
	"name": "pattern-platform",
	"version": "1.0.0",
	"private": true,
	"license": "GPL-3.0",
	"scripts": {
		"format": "prettier --write \"**/*.{js,json,md,mdx}\"",
		"test:eslint": "eslint . --ext js",
		"test:prettier": "prettier --list-different \"**/*.{js,json,md,mdx}\"",
		"test": "yarn test:prettier && yarn test:eslint",
		"dev": "next dev",
		"build": "next build",
		"export": "next export",
		"start": "next start",
		"lint": "next lint",
		"nuke": "rm -rf node_modules && rm -rf .next rm -rf out && rm yarn.lock"
	},
	"dependencies": {
		"@babel/runtime": "7.19.0",
		"@emotion/babel-plugin": "11.10.2",
		"@emotion/react": "11.10.4",
		"avataaars": "^2.0.0",
		"next": "12.3.0",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@emotion/eslint-plugin": "11.10.0",
		"babel-eslint": "10.1.0",
		"eslint": "8.23.1",
		"eslint-config-next": "12.3.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-react": "7.31.8",
		"prettier": "2.7.1",
		"yarn": "1.22.19"
	}
}
